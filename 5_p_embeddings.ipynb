    "def format_podcast_data_to_string_for_embedding(pod_data):\n",
    "    pod_stats = pod_data.get(\"pod_stats\", {})\n",
    "    sponsor_stats = pod_data.get(\"pod_sponsors_stats\", {})\n",
    "    subcategories = pod_data.get(\"subcategory\", [])\n",
    "\n",
    "\n",
    "    if isinstance(pod_stats, str):\n",
    "        pod_stats = ast.literal_eval(pod_stats)\n",
    "\n",
    "    if isinstance(pod_sponsors_stats, str):\n",
    "        sponsor_stats = ast.literal_eval(sponsor_stats)\n",
    "\n",
    "    estimated_ad_price = pod_stats.get(\"estimated_ad_price\", {})\n",
    "    if isinstance(estimated_ad_price, str):\n",
    "        estimated_ad_price = ast.literal_eval(estimated_ad_price)\n",
    "\n",
    "    s30price = estimated_ad_price.get(\"30s\", \"unknown\")\n",
    "    s60price = estimated_ad_price.get(\"60s\", \"unknown\")\n",
    "\n",
    "\n",
    "    def safe_int(v):\n",
    "        try:\n",
    "            return int(float(v))\n",
    "        except:\n",
    "            return 0\n",
    "\n",
    "    def safe_float(v, digits=2):\n",
    "        try:\n",
    "            return round(float(v), digits)\n",
    "        except:\n",
    "            return 0.0\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "    return f\"\"\"\n",
    "Podcast Summary: {pod_data.get(\"summary\", \"\")}\n",
    "Main Category: {pod_data.get(\"category\", \"\")}\n",
    "Subcategories: {\", \".join(subcategories)}\n",
    "\n",
    "YouTube Subscribers: {safe_int(pod_stats.get(\"youtube_subscribers\"))}\n",
    "Instagram Followers: {safe_int(pod_stats.get(\"instagram_followers\"))}\n",
    "Average Episode Views: {safe_int(pod_stats.get(\"episode_avg_views\"))}\n",
    "Impressions Range: {safe_int(pod_stats.get(\"min_impressions\"))} to {safe_int(pod_stats.get(\"max_impressions\"))}\n",
    "Estimated Ad Price for 30s: {s30price}$\n",
    "Estimated Ad Price for 60s: {s60price}$\n",
    "\n",
    "Episodes with Ads: {safe_float(sponsor_stats.get(\"percentage_episodes_with_ads\"))}%\n",
    "Average Sponsor Length: {safe_int(sponsor_stats.get(\"average_sponsor_length\"))} seconds\n",
    "Ad Percentage Per Episode: {safe_float(sponsor_stats.get(\"average_ad_percentage_per_episode\"))}%\n",
    "Average Ads Per Episode: {safe_float(sponsor_stats.get(\"avg_episode_ad_count\"), 1)}\n",
    "Brand Repeat Rate: {safe_float(sponsor_stats.get(\"average_brand_repeat_rate\"), 2)}\n",
    "Top Past Sponsors: {\", \".join(sponsor_stats.get(\"top_brands\", []))}\n",
    "\n",
    "\"\"\".strip()"
   ]
