    "def filter_unique_sponsors(sponsors):\n",
    "    unique_sponsors = []\n",
    "    sponsor_names = set()\n",
    "\n",
    "    for sponsor in sponsors:\n",
    "        # Create a unique key combining sponsorName, episodeId, startTime, and endTime\n",
    "        brand_name = (\n",
    "            (sponsor.get(\"sponsorName\", \"\").lower()) +\n",
    "            str(sponsor.get(\"episodeId\", \"\")) +\n",
    "            str(sponsor.get(\"startTime\", \"\")) +\n",
    "            str(sponsor.get(\"endTime\", \"\"))\n",
    "        )\n",
    "\n",
    "        if brand_name not in sponsor_names:\n",
    "            sponsor_names.add(brand_name)\n",
    "            unique_sponsors.append(sponsor)\n",
    "\n",
    "    return unique_sponsors\n",
