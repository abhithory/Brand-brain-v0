    "def category_len(cat):\n",
    "    if pd.isna(cat):  # Handle NaN\n",
    "        return 0\n",
    "    elif isinstance(cat, list):  # If it's a list/array\n",
    "        return len(cat)\n",
    "    elif isinstance(cat, str):  # If it's a single string\n",
    "        return len([c for c in cat.split(',') if c.strip()])  # Ignore empty strings\n",
    "\n",
    "    else:\n",
    "        return -2\n",
    "\n",
    "categories_count = []\n",
    "for pod in pods:\n",
    "    categories_count = pods[\"categories\"].apply(category_len)\n",
    "print(\"all have categories: \",categories_count.tolist())\n"
   ]
